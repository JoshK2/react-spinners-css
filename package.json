{
  "name": "react-spinners-css",
  "version": "1.1.7",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JoshK2/react-spinners-css"
  },
  "homepage": "https://bit.dev/joshk/react-spinners-css",
  "keywords": [
    "react-spinners",
    "react-spinner",
    "loadingio",
    "loading-animations",
    "loaders",
    "css-animations",
    "reactjs",
    "component",
    "react-component",
    "react",
    "spinner",
    "spinners",
    "loading",
    "loader",
    "progress",
    "activity"
  ],
  "author": "JoshK2",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build:lib": "rm -rf dist && NODE_ENV=production babel src/components --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build:bit": "rm -rf dist && bit build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "deploy:npm": "npm publish",
    "deploy:bit": "bit export joshk.react-spinners-css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@bit/joshk.jotils.get-random-color": "^2.0.8",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.16"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
